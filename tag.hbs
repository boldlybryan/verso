{{!< default}}

{{#tag}}
    <h1>{{name}}</h1>
    <p>{{description}}</p>
{{/tag}}

<main role="main">
<!-- This is the post loop - each post will be output using this markup -->
  {{#foreach posts}}
	<article class="{{post_class}}">
        <h2><a href="{{url}}">{{title}}</a></h2>
        {{> "publish-stamp"}}
        <section>
            <p>{{excerpt}}</p>
            <a href="{{url}}">Permalink</a>
        </section>
    </article>
  {{/foreach}}

</main>

{{pagination}}